{
  "expo": {
    "name": "MyFoodPlanner",
    "slug": "MyFoodPlanner",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/background.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kraenbyskov.MyFoodPlanner"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.kraenbyskov.MyFoodPlanner"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
